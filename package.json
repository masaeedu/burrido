  "license": "MIT",
  "dependencies": {
    "immutagen": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "mocha": "2.4.5"
  },
  "keywords": [
    "monad",
    "do",
    "notation",
    "syntax",
